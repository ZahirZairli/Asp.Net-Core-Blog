<!DOCTYPE HTML>
<html lang="az">
<head>
    <meta charset="UTF-8">
    <title>@ViewBag.Title</title>
    <script data-ad-client="ca-pub-3039898546903699" async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="keywords" content="pulsuz pdf yüklə,edufıle,edufilestock,pdf,pulsuz pdf,pulsuz kitab,kitab,bedii kitab,alman dili,ingilis dili,ielts,rezidentura,magistr,master,azerbaijan,turkey,heyder aliyev,ilham aliyev,qarabaq,messi,ronaldo,zairli zahir,facebook,instagram,hacker,hacking,oyun hiyleleri,heyvanistan,faydali melumatlar,cizgi filmler,olasiliksiz,film,animasya,rejissor,avatar,dirilis ertuqrul,kurulus osman" />
    <meta name="og:title" content="@ViewBag.ogtitle">
    <meta name="description" content="@ViewBag.description" />
    <link type="text/css" rel="stylesheet" href="~/Content/css/plugins.css">
    <link type="text/css" rel="stylesheet" href="~/Content/css/style.css">
    <link type="text/css" rel="stylesheet" href="~/Content/css/color.css">
    <link rel="shortcut icon" href="~/Content/images/lo.jpeg">
</head>
<body>
    <div class="loader">
        <div class="loading-text-container "><span class="loading-text">Load<strong>ing</strong></span> <span class="loader_count">0</span></div>
        <div class="loader-anim"></div>
        <div class="loader-anim2 color-bg"></div>
    </div>
    <div id="main">
        <header class="main-header">
            <a href="~/Site/index" class="ajax logo-holder"><img src="~/Content/images/lo.jpeg" alt="edufilestock"></a>
            <div class="nav-button but-hol">
                <span class="ncs"></span>
                <span class="nos"></span>
                <span class="nbs"></span>
                <div class="menu-button-text">Menu</div>
            </div>
            <div class="header-contacts">
                <ul>
                    <li><span>01. Zəng et </span> <a href="tel:+994513551006">+994513551006</a></li>
                    <li><span>02. Yaz </span> <a href="mailto:zairli.zahir1@gmail.com">zairli.zahir1@gmail.com</a></li>
                </ul>
                <a href="~/Site/contact" class="ajax contacts-btn">Get in touch</a>
            </div>
        </header>
        <aside class="left-header">
            <span class="lh_dec color-bg"></span>
            <div class="left-header_social">
                <ul>
                    <li><a href="https://www.facebook.com/edufilestock" target="_blank"><i class="fab fa-facebook-f"></i></a></li>
                    <li><a href="https://www.instagram.com/edufilestock/" target="_blank"><i class="fab fa-instagram"></i></a></li>
                    <li><a href="#" target="_blank"><i class="fab fa-twitter"></i></a></li>
                    <li><a href="#" target="_blank"><i class="fab fa-vk"></i></a></li>
                </ul>
            </div>
        </aside>
        <div class="share-btn showshare color-bg"><span>Share <i class="fal fa-plus"></i></span></div>
        <div class="hc_dec_color">
            <div class="page-subtitle"><span></span></div>
        </div>
        <div id="wrapper">
            <div class="nav-holder">
                <div class="nav-holder-wrap but-hol" style="overflow-y: scroll;background-color: rgba(49, 49, 49, 0.9)">
                    <div class="nav-container fl-wrap" style="height: 60vh !important;">
                        <nav class="nav-inner" id="menu">
                            <ul>
                                <li>
                                    <a href="~/Site/Index" class="act-link">Ana səhifə</a>
                                </li>
                                <li><a href="~/Site/about" class="ajax">Haqqımızda</a></li>
                                <li>
                                    <a href="#">MATERIALLAR</a>
                                    <ul id="myDropdown">
                                        @foreach (var item in ViewBag.melumat)
                                        {
                                            <li class="liz">
                                                <a href="" class="link">@item.MelumatAd</a>
                                                <ul>
                                                    @foreach (var item2 in ViewBag.melumatalt)
                                                    {
                                                        if (item.MelumatId == item2.MelumatAltLiMelumatId)
                                                        {
                                                            <li><a href="~/Site/detail/@item2.MelumatAltLiId/?@item2.MelumatAltLiAd" class="ajax">@item2.MelumatAltLiAd</a></li>
                                                        }
                                                    }
                                                </ul>
                                            </li>
                                        }
                                    </ul>
                                </li>
                                <li><a href="~/Site/contact" class="ajax">Əlaqə</a></li>
                            </ul>
                        </nav>
                    </div>
                    @*<div style="margin-bottom: 0px !important;">

                    <div class="nav-footer"><span>&#169; EduFileStock  /  Created by <a href="https://www.instagram.com/zairlizahir/">Zahir Zairli</a></span></div>
                    <div class="nav-holder-wrap_line"></div>
                    <div class="nav-holder-wrap_dec"></div>
                    </div>*@
                </div>
            </div>
            <div class="nav-overlay"></div>
            @RenderBody()
            <div class="share-wrapper">
                @*<div class="close-share-btn"><i class="fal fa-long-arrow-left"></i></div>*@
                @*<div class="share-container fl-wrap"></div>*@
            </div>
        </div>
        <div class="element">
            <div class="element-item"></div>
        </div>
    </div>
    <script src="~/Content/js/jquery.min.js"></script>
    <script src="~/Content/js/plugins.js"></script>
    <script src="~/Content/js/scripts.js"></script>
</body>
</html>






















