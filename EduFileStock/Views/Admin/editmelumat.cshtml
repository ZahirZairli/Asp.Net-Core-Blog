
@{
    ViewBag.Title = "editmelumat";
    Layout = "~/Views/Shared/Admin.cshtml";
}

@using EduFileStock.Models
<div class="container-fluid">
    <div class="content">
        <form method="post" action="~/Admin/upmelumat/@Model.MelumatId">
            <div class="row">
                <div class="col-md-12">
                    <div class="form-group">
                        <label><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Kateqoryanın yeni adı:</font></font></label>
                        <input type="text" class="form-control" placeholder="ad" name="MelumatAd" value="@Model.MelumatAd">
                    </div>
                </div>
                <button type="submit" class="btn btn-info btn-fill pull-right"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Yadda saxla</font></font></button>
                <div class="clearfix"></div>
            </div>
        </form>
    </div>
</div>

